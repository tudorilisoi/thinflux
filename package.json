{
  "name": "thinflux",
  "version": "0.1.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/tudorilisoi/thinflux"
  },
  "description": "A reactive JS store for managing tree-shaped (deep, nested) state",
  "main": "./dist/index.js",
  "browser": "./dist/browser/Store.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist 2>&1; mkdir -p dist/browser",
    "build-babel": "npx babel src --out-dir dist",
    "build-browser": "browserify dist/Store.js > dist/browser/Store.js && uglifyjs dist/browser/Store.js -c -m -o dist/browser/Store.min.js",
    "build": "npm run clean && npm run build-babel && npm run build-browser"
  },
  "keywords": [
    "JS",
    "React",
    "Flux",
    "store",
    "Redux",
    "Redux alternative"
  ],
  "author": "Tudor Ilisoi",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "browserify": "^16.2.2",
    "react": "^16.3.2"
  },
  "dependencies": {
    "cuid": "^2.1.1",
    "lodash": "^4.17.10",
    "uglify-js": "^3.3.25"
  }
}
